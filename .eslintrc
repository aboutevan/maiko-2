{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "global-require": 0,
    "react/no-danger": 0,
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }]
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "foundation": true
  }
}